{
  "name": "@magda/minion-visualization",
  "description": "MAGDA visualization Minion",
  "version": "0.0.57-0",
  "scripts": {
    "prebuild": "rimraf dist tsconfig.tsbuildinfo",
    "build": "yarn run compile",
    "compile": "tsc -b",
    "watch": "tsc -b --watch",
    "start": "node dist/index.js",
    "dev": "run-typescript-in-nodemon src/index.ts",
    "docker-build-local": "create-docker-context-for-node-component --build --push --tag auto --local",
    "docker-build-prod": "create-docker-context-for-node-component --build --push --tag auto",
    "retag-and-push": "retag-and-push",
    "test": "mocha",
    "helm-lint": "helm lint deploy/magda-minion-visualization -f deploy/test-deploy.yaml"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@magda/docker-utils": "^2.0.0",
    "@types/chai": "^4.3.3",
    "@types/express": "4.17.9",
    "@types/lodash": "^4.14.185",
    "@types/mocha": "^9.1.1",
    "@types/read-pkg-up": "^3.0.1",
    "@types/urijs": "^1.19.19",
    "chai": "^4.3.6",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@magda/minion-sdk": "^2.0.0",
    "@magda/registry-aspects": "^2.0.0",
    "csv-parse": "^5.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.19.1",
    "node-fetch": "^3.2.10",
    "read-pkg-up": "^3.0.0",
    "urijs": "^1.19.11"
  },
  "resolutions": {
    "@types/express": "4.17.9"
  },
  "config": {
    "registryUrl": "http://localhost:6101/v0",
    "docker": {
      "name": "data61/magda-minion-visualization",
      "include": "node_modules dist Dockerfile package.json"
    },
    "jwtSecret": "squirrel",
    "userId": "00000000-0000-4000-8000-000000000000"
  },
  "magda": {
    "language": "typescript",
    "categories": {
      "minion": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
